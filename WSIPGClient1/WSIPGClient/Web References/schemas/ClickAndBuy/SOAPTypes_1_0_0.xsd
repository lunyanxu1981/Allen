  <xs:simpleType name="DebitCardNumber">
    <xs:annotation>
      <xs:documentation>debit card number</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="0"/>
      <xs:maxLength value="64"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="Department">
    <xs:annotation>
      <xs:documentation>department of a company</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="0"/>
      <xs:maxLength value="100"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="Digest">
    <xs:annotation>
      <xs:documentation>message digest</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="hmac" type="cab:HMAC">
        <xs:annotation>
          <xs:documentation>the keyed-hashed message authentication code (hex)</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="DigestElement">
    <xs:annotation>
      <xs:documentation>digest element(s) (XPath)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:complexType name="DomesticAccount">
    <xs:annotation>
      <xs:documentation>domestic account</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element name="accountNumber" type="cab:AccountNumber">
        <xs:annotation>
          <xs:documentation>account number</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="bankZipCode" type="cab:BankZipCode">
        <xs:annotation>
