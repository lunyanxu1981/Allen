<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cab="http://api.clickandbuy.com/webservices/pay_1_0_0/" attributeFormDefault="qualified" elementFormDefault="qualified" targetNamespace="http://api.clickandbuy.com/webservices/pay_1_0_0/">
  <xs:complexType name="AccountDetails">
    <xs:annotation>
      <xs:documentation>account details</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element minOccurs="0" name="domesticAccount" type="cab:DomesticAccount">
        <xs:annotation>
          <xs:documentation>domestic account</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="internationalAccount" type="cab:InternationalAccount">
        <xs:annotation>
          <xs:documentation>international account</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:simpleType name="AccountHolder">
    <xs:annotation>
      <xs:documentation>account holder of a bank account</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="0"/>
      <xs:maxLength value="255"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="AccountIdentificationNumber">
    <xs:annotation>
      <xs:documentation>identification number (mandatory for some countries)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="0"/>
      <xs:maxLength value="16"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="AccountLimit">
    <xs:annotation>
      <xs:documentation>account limit</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:maxInclusive value="999999"/>
      <xs:fractionDigits value="2"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="AccountNumber">
    <xs:annotation>
      <xs:documentation>account number</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="0"/>
      <xs:maxLength value="30"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="Address">
    <xs:annotation>
      <xs:documentation>address data</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="street" type="cab:Street">
        <xs:annotation>
          <xs:documentation>name of street, may include house number</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="houseNumber" type="cab:HouseNumber">
        <xs:annotation>
          <xs:documentation>house number</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="houseNumberSuffix" type="cab:HouseNumberSuffix">
        <xs:annotation>
          <xs:documentation>house number suffix (e.g. floor)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="zip" type="cab:Zip">
        <xs:annotation>
          <xs:documentation>zip code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="city" type="cab:City">
        <xs:annotation>
          <xs:documentation>city of the registry court</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="country" type="cab:Country">
        <xs:annotation>
          <xs:documentation>ISO 3166-1 alpha-2 code of country</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="state" type="cab:State">
        <xs:annotation>
          <xs:documentation>state</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="addressSuffix" type="cab:AddressSuffix">
        <xs:annotation>
          <xs:documentation>address suffix (e.g. c/o)</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="AddressSuffix">
    <xs:annotation>
      <xs:documentation>address suffix (e.g. c/o)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="0"/>
      <xs:maxLength value="255"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="Age">
    <xs:annotation>
      <xs:documentation>age</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:byte">
      <xs:minInclusive value="0"/>
      <xs:maxInclusive value="127"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="Amount">
    <xs:annotation>
      <xs:documentation>amount</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:maxInclusive value="9999999999999999"/>
      <xs:fractionDigits value="3"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="BIC">
    <xs:annotation>
      <xs:documentation>Bank Identifier Code (BIC)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="8"/>
      <xs:maxLength value="11"/>
      <xs:pattern value="[A-Za-z]{4}[A-Za-z]{2}[0-9A-Za-z]{2,5}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="BankAccount">
    <xs:annotation>
      <xs:documentation>bank account</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="country" type="cab:Country">
        <xs:annotation>
          <xs:documentation>ISO 3166-1 alpha-3 code of country</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="bankName" type="cab:BankName">
        <xs:annotation>
          <xs:documentation>bank name</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="accountHolder" type="cab:AccountHolder">
        <xs:annotation>
          <xs:documentation>account holder name</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="bankAccountType" type="cab:BankAccountType">
        <xs:annotation>
          <xs:documentation>type of the bank account
          DOMESTIC ... domestic bank account only
          INTERNATIONAL ... internationbal bank account only
          BOTH ... domestic and international bank account</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="limit" type="cab:AccountLimit">
        <xs:annotation>
          <xs:documentation>account limit</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="accountDetails" type="cab:AccountDetails">
        <xs:annotation>
          <xs:documentation>account details</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="BankAccountType">
    <xs:annotation>
      <xs:documentation>type of the bank account
      DOMESTIC ... domestic bank account only
      INTERNATIONAL ... internationbal bank account only
      BOTH ... domestic and international bank account</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="DOMESTIC"/>
      <xs:enumeration value="INTERNATIONAL"/>
      <xs:enumeration value="BOTH"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="BankName">
    <xs:annotation>
      <xs:documentation>bank name</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="0"/>
      <xs:maxLength value="255"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="BankStatement">
    <xs:annotation>
      <xs:documentation/>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="0"/>
      <xs:maxLength value="255"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="BankZipCode">
    <xs:annotation>
      <xs:documentation>bank zip code</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="0"/>
      <xs:maxLength value="30"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="Boolean">
    <xs:annotation>
      <xs:documentation>boolean flag</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:boolean"/>
  </xs:simpleType>
  <!-- xs:complexType name="Brand">
    <xs:annotation>
      <xs:documentation/>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="id" type="xs:long">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:element>
      <xs:element name="shortName" type="xs:string">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:element>
      <xs:element name="label" type="xs:string">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType-->
  <xs:simpleType name="CAVV_UCAF">
    <xs:annotation>
      <xs:documentation>Cardholder Authentification Varification Value (Visa) or Universal Cardholder Authentication Field (Mastercard)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="CSC">
    <xs:annotation>
      <xs:documentation>Card Security Code (CVC, CVV, CID)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="3"/>
      <xs:maxLength value="4"/>
      <xs:pattern value="\d{3,4}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CardHolder">
    <xs:annotation>
      <xs:documentation>card holder of a credit or debit card</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="0"/>
      <xs:maxLength value="255"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="City">
    <xs:annotation>
      <xs:documentation>city</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="0"/>
      <xs:maxLength value="100"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="Comment">
    <xs:annotation>
      <xs:documentation>comment</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="0"/>
      <xs:maxLength value="255"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="Company">
    <xs:annotation>
      <xs:documentation>company</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="name" type="cab:CompanyName">
        <xs:annotation>
          <xs:documentation>name of the company</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="vatID" type="cab:VatID">
        <xs:annotation>
          <xs:documentation>value added tax ID</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="registry" type="cab:Registry">
        <xs:annotation>
          <xs:documentation>commercial registry data</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="address" type="cab:Address">
        <xs:annotation>
          <xs:documentation>address</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="CompanyName">
    <xs:annotation>
      <xs:documentation>name of a company</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="0"/>
      <xs:maxLength value="255"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="Consumer">
    <xs:annotation>
      <xs:documentation>end customer data (shopper's data)</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="salutation" type="cab:Salutation">
        <xs:annotation>
          <xs:documentation>salutation
          MR ... Mister
          MS ... Misses</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="title" type="cab:Title">
        <xs:annotation>
          <xs:documentation>Title</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="firstName" type="cab:FirstName">
        <xs:annotation>
          <xs:documentation>first name</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="lastName" type="cab:LastName">
        <xs:annotation>
          <xs:documentation>last name</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="maidenName" type="cab:LastName">
        <xs:annotation>
          <xs:documentation>maiden name</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="gender" type="cab:Gender">
        <xs:annotation>
          <xs:documentation>gender
          UNKNOWN ... unknown
          MALE ... male
          FEMALE ... femal</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="dateOfBirth" type="cab:Date">
        <xs:annotation>
          <xs:documentation>date of birth</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="language" type="cab:Language">
        <xs:annotation>
          <xs:documentation>ISO 639-3 language code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="address" type="cab:Address">
        <xs:annotation>
          <xs:documentation>address</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="ConsumerContext">
    <xs:annotation>
      <xs:documentation>the consumer's context (WEB, MOBILE, etc.)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="0"/>
      <xs:maxLength value="100"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="ContactData">
    <xs:annotation>
      <xs:documentation>data of a contact person</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element maxOccurs="unbounded" name="element" type="cab:ContactDataElement">
        <xs:annotation>
          <xs:documentation>contact data element</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ContactDataElement">
    <xs:annotation>
      <xs:documentation>contact data element</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element name="item" type="cab:ContactDataElementItem">
        <xs:annotation>
          <xs:documentation>contact data element item/name</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="value" type="cab:ContactDataElementValue">
        <xs:annotation>
          <xs:documentation>contact data element value</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="type" type="cab:ContactDataElementType">
        <xs:annotation>
          <xs:documentation>contact data element type</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="pattern" type="cab:ContactDataElementPattern">
        <xs:annotation>
          <xs:documentation>contact data element pattern</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:simpleType name="ContactDataElementItem">
    <xs:annotation>
      <xs:documentation>contact data element item/name</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="type"/>
      <xs:enumeration value="department"/>
      <xs:enumeration value="phone"/>
      <xs:enumeration value="mobile"/>
      <xs:enumeration value="fax"/>
      <xs:enumeration value="ip"/>
      <xs:enumeration value="email"/>
      <xs:enumeration value="www"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ContactDataElementPattern">
    <xs:annotation>
      <xs:documentation>contact data element pattern</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="ContactDataElementType">
    <xs:annotation>
      <xs:documentation>contact data element type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="ContactType"/>
      <xs:enumeration value="Department"/>
      <xs:enumeration value="Phone"/>
      <xs:enumeration value="IPAddress"/>
      <xs:enumeration value="Email"/>
      <xs:enumeration value="URL"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ContactDataElementValue">
    <xs:annotation>
      <xs:documentation>contact data element value</xs:documentation>
    </xs:annotation>
    <xs:union memberTypes="cab:ContactType cab:Department cab:Phone cab:IPAddress cab:Email cab:URL"/>
  </xs:simpleType>
  <xs:simpleType name="ContactType">
    <xs:annotation>
      <xs:documentation>type of contact
      PERSON ... person
      LEGAL_ENTITY ... legal entity</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="PERSON"/>
      <xs:enumeration value="LEGAL_ENTITY"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="Count">
    <xs:annotation>
      <xs:documentation>count</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:minInclusive value="0"/>
      <xs:maxInclusive value="2147483647"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="Country">
    <xs:annotation>
      <xs:documentation>ISO 3166-1 alpha-2 code of country</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="2"/>
      <xs:maxLength value="2"/>
    </xs:restriction>
  </xs:simpleType>
