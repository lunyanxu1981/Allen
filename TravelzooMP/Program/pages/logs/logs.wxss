@import "/pages/test/test.wxss";
page {
  background-color: #fff;
}
.our{
  background: #fff;
  text-align: center;
  padding:20rpx 0;
}
ul{
  padding: 0 22rpx;
  display: flex;
  font-size: 26rpx;
  color:#808080;
  flex-direction: column;
}
li{
  width: 100%;
  display: flex;
  align-items: center;
  height: 100rpx;
   flex-direction: row; 
  border-bottom: 1px solid #e5e5e5;
  
}
.priceCen{
  width: 100%;
  flex: 1;
  font-weight: bold;
  color: #000;
  font-size: 24px;
}
.table-top{
  text-align: left;
  padding: 20rpx 22rpx 0 22rpx;
  font-size: 26rpx;
  color:#808080;
  border-top:1px solid #e5e5e5;
}
.color1{
  color:#38da71;
  font-size: 22px;
}
.color2{
  color:#0276ee;
  font-size: 22px;
}
.mt20mb30{
  margin: 20rpx 0 60rpx;
}
.get{
  display:flex;
  flex:1;
}
button[type="primary"]{
  width: 93%;
  background: #0276ee !important;
}
.tips{
  text-align: center;
  font-size: 26rpx;
  color:#000;
  padding:20rpx 0;
}
.coupon{
   height: 40px;
   line-height: 40px;
   font-size: 26rpx;
   color:#000;
   border-top:1px solid #e5e5e5;
   border-bottom:1px solid #e5e5e5;
   margin-top: 34rpx;
   text-align:left;

}
.coupon image{
  width:20px;
  height:20px;
  position:relative;
  left:12px;
  top:5px;
}
.utitle{
   position:relative;
   left:20px;
}
.upic{
  position:absolute;
  right:18px;
  text-align:right;
  display:inline-block;

}
.cred {
  color: #f00;
  font-size: 32rpx;
  margin: 20rpx 0 40rpx;
  text-align: center;
}
