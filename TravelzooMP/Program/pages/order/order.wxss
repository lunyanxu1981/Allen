.car_item_wrap{
  position: relative;
  height: 100px;
  background: #fff;
  padding-left: 140px;
  margin: 20rpx 0;
}
.car_image_wrap{
  position: absolute;
  top: 10px;
  left: 10px;
}
.car_item_image{
  width: 120px;
  height: 80px;
}
.car_image_title{
  padding: 10px 10px 10px 0;
  font-size: 14px;
}
.car_item_bot{
  padding: 0 10px 0 0;
  height: 28px;
  line-height: 28px;
}
.car_item_day{
  font-size: 14px;
}
.car_item_hui{
  font-size: 14px;
  color: #e9e9e9;
}
.txt-cor{
  color:#e9e9e9;
}
.nogoods{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  position: fixed;
  width: 100%;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  font-size: 32rpx;
  color: #888;
}
.txt-show {
  text-align: center;
  font-size: 14px;
}
