		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Secure3DAuthenticationRequest">
		<xs:sequence>
			<xs:choice>
				<xs:element name="AcsResponse" type="v1:AcsResponse" />
				<xs:element name="IVRAuthenticationRequest" type="v1:IVRAuthenticationRequest" />
			</xs:choice>
		</xs:sequence>
	</xs:complexType>

