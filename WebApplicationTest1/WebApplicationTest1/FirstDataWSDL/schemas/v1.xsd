	<xs:complexType name="Secure3DVerificationResponse">
		<xs:sequence>
			<xs:element name="VerificationRedirectResponse" type="v1:VerificationRedirectResponse" minOccurs="0" />
			<xs:element name="IVRVerificationResponse" type="v1:IVRVerificationResponse" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="VerificationRedirectResponse">
		<xs:sequence>
			<xs:element name="AcsURL" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						target of Secure3d redirection
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaReq" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						PaReq data which has to be send in the "PaReq" attribute to the AcsURL.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TermUrl" type="xs:string">
				<xs:annotation>
					<xs:documentation>Default IPG TermURL which should be used in order to process
						the response from Secure3d in IPG. In case that you would like to parse the
						response by yourself you have to specify the "TermUrl" param in form with
						your custom URL in which you will process the response and call the API with
						the response PaRes and MD (Merchant Data)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MD" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						Merchant Data which has to be send in the "MD" attribute to the AcsURL.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="IVRVerificationRequest">
		<xs:sequence>
			<xs:element name="IVRDeviceIdFormat" type="v1:IVRDeviceIdFormat" />
			<xs:element name="IVRDeviceId" type="v1:PositiveNumeric25max" />
