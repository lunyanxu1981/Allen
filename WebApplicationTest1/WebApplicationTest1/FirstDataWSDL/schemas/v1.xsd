			<xs:element name="IVRAuthenticationChannel" type="v1:IVRAuthenticationChannel"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="IVRVerificationResponse">
		<xs:sequence>
			<xs:element name="IVRAuthData" type="v1:IVRAuthData" minOccurs="1" maxOccurs="99" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="AcsResponse">
		<xs:annotation>
			<xs:documentation>
				use this element to populate the response data ACS redirect to the payment gateway
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="MD" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						Merchant Data from Acs redirection POST attribute ("MD" attribute).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaRes" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						PaRes data from Acs redirection POST attribute ("PaRes" attribute).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

