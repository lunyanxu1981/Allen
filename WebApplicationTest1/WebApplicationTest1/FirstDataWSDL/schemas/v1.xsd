				<xs:element name="CashbackAmount" type="v1:AmountValueType" minOccurs="0" />
				<xs:element name="TipAmount" type="v1:AmountValueType" minOccurs="0" />
			</xs:sequence>
			<!-- ChargeTotal = SubTotal + ValueAddedTax + localTax + DeliveryAmount + CashbackAmount + TipAmount -->
			<xs:element name="ChargeTotal" type="v1:AmountValueType" />
			<xs:element name="Currency" type="v1:CurrencyType">

			</xs:element>
		</xs:sequence>
	</xs:group>

	<xs:group name="ItemAmount">
		<xs:sequence>
			<xs:sequence minOccurs="0">
				<xs:element name="SubTotal" type="v1:ItemAmountValueType" />
				<xs:element name="ValueAddedTax" type="v1:ItemAmountValueType" minOccurs="0" />
				<xs:element name="localTax" type="v1:ItemAmountValueType" minOccurs="0" />
				<xs:element name="DeliveryAmount" type="v1:ItemAmountValueType" minOccurs="0" />
			</xs:sequence>
			<!-- ChargeTotal = SubTotal + ValueAddedTax + localTax + DeliveryAmount -->
			<xs:element name="ChargeTotal" type="v1:ItemAmountValueType" />
			<xs:element name="Currency" type="v1:CurrencyType">
				<xs:annotation>
					<xs:documentation>Currency will be ignored by IPG</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>

	<xs:complexType name="TransactionDetails">
		<xs:all>
			<xs:element name="AirlineDetails" type="v1:AirlineDetails" minOccurs="0" />
			<xs:element ref="v1:AdditionalRequestParameters" minOccurs="0" />
			<xs:element name="CarRental" type="v1:CarRental" minOccurs="0" />
			<xs:element name="HotelLodging" type="v1:HotelLodging" minOccurs="0" />
			<xs:element name="PurchaseCard" type="v1:PurchaseCard" minOccurs="0" />
			<xs:element name="Comments" type="v1:String1024max" minOccurs="0" />
			<xs:element name="InvoiceNumber" type="v1:String48max" minOccurs="0" />
			<xs:element name="DynamicMerchantName" type="v1:String100max" minOccurs="0" />
			<xs:element name="PONumber" type="v1:String128max" minOccurs="0" />
			<xs:element name="OrderId" type="v1:String100max" minOccurs="0" />
			<xs:element name="MerchantTransactionId" type="v1:String40max" minOccurs="0" />
			<xs:element name="ReferencedMerchantTransactionId" type="v1:String40max" minOccurs="0" />
			<xs:element name="ReferencedSchemeTransactionId" type="v1:String40max" minOccurs="0" />
			<xs:element name="IpgTransactionId" type="v1:PositiveNumeric14max" minOccurs="0" />
			<xs:element ref="v1:OfflineApprovalType" minOccurs="0" />
			<xs:element name="Ip" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="((25[0-5]|(2[0-4]|1[0-9]|[1-9])?[0-9])\.){3}(25[0-5]|(2[0-4]|1[0-9]|[1-9])?[0-9])|" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ReferenceNumber" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="(NEW)?[0-9a-zA-Z]{1,8}" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="SplitShipment" type="v1:SplitShipment" minOccurs="0" />
			<xs:element name="TDate" type="v1:TDateType" minOccurs="0" />
			<xs:element ref="v1:TransactionOrigin" minOccurs="0" />
			<xs:element ref="v1:Terminal" minOccurs="0" />
			<xs:element name="InquiryRateReference" type="v1:InquiryRateReference" minOccurs="0" />
			<!-- please provide the signature in PNG format -->
			<xs:element name="Signature" type="xs:base64Binary" minOccurs="0" />
		</xs:all>
	</xs:complexType>

	<xs:element name="AdditionalRequestParameters">
		<xs:complexType>
			<xs:sequence>
				<xs:element maxOccurs="10" ref="v1:keyValuePair"/>
			</xs:sequence>
		</xs:complexType>
		<xs:unique name="uniqueKey">
			<xs:selector xpath="v1:keyValuePair"/>
			<xs:field xpath="v1:key"/>
		</xs:unique>
	</xs:element>
	<xs:element name="keyValuePair">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="key">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1" />
							<xs:maxLength value="100" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="value">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1" />
							<xs:maxLength value="100" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="PurchaseCard">
		<xs:annotation>
			<xs:documentation>Please use the Payment element of the transaction to advise the ValueAddedTax (relevant in the context) or Shipping amount.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CustomerReferenceID" type="v1:String20max">
				<xs:annotation>
					<xs:documentation>Customer Code / Customer Reference ID</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SupplierInvoiceNumber" type="v1:String30max">
				<xs:annotation>
					<xs:documentation>Purchase Identifier/Merchant related data</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SupplierVATRegistrationNumber" type="v1:String30max">
				<xs:annotation>
					<xs:documentation>Merchant VAT registration/Single Business Reference Number/Merchant Tax ID or Corporation VAT Number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TotalDiscountAmountAndRate" type="v1:AdditionalAmountAndRate" minOccurs="0" />
			<xs:element name="VATShippingAmountAndRate" type="v1:AdditionalAmountAndRate" minOccurs="0">
				<xs:annotation>
					<xs:documentation>VAT amount/rate that is owed for shipping charges</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="v1:LineItemData" minOccurs="0" maxOccurs="100" />
		</xs:sequence>
	</xs:complexType>

	<xs:element name="LineItemData">
		<xs:complexType>
			<xs:all>
				<xs:element name="CommodityCode" type="v1:PositiveNumeric4max">
					<xs:annotation>
						<xs:documentation>The commodity code used to classify the item purchased</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ProductCode" type="v1:String20max" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Merchant product identifier/The Universal Product Code (UPC) of the item purchased</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Description" type="v1:String30max" />
				<xs:element name="Quantity">
					<xs:simpleType>
						<xs:restriction base="xs:int">
							<xs:minInclusive value="1" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="UnitOfMeasure" type="v1:String3max" />
				<xs:element name="UnitPrice" type="v1:AmountValueType" />
				<xs:element name="VATAmountAndRate" type="v1:AdditionalAmountAndRate" minOccurs="0" />
				<xs:element name="DiscountAmountAndRate" type="v1:AdditionalAmountAndRate" minOccurs="0" />
				<xs:element name="LineItemTotal" type="v1:AmountValueType" />
			</xs:all>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="CarRental">
		<xs:sequence>
			<xs:element name="AgreementNumber" type="v1:String20max" />
			<xs:element name="RenterName" type="v1:String20max" />
			<xs:element name="ReturnCity" type="v1:String20max" />
			<xs:element name="ReturnDate" type="v1:StringDate" />
			<xs:element name="PickupDate" type="v1:StringDate" />
			<xs:element name="RentalClassID" type="v1:String6max" />
			<xs:element name="ExtraCharges" minOccurs="0" maxOccurs="unbounded">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Extra Mileage" />
						<xs:enumeration value="Gas" />
						<xs:enumeration value="Late Return" />
						<xs:enumeration value="One Way Ser Fee" />
						<xs:enumeration value="Parking Violation" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="NoShowIndicator" type="xs:boolean" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:element name="Mandate">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Reference" type="v1:MandateReferenceType" minOccurs="0" />
				<xs:element name="Type" type="v1:MandateType" minOccurs="1" default="SINGLE" />
				<xs:element name="Date" type="v1:StringDate" minOccurs="0"/>
				<xs:element name="Url" type="v1:String100max" minOccurs="0" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="HotelLodging">
		<xs:sequence>
			<xs:element name="ArrivalDate" type="v1:StringDate" />
			<xs:element name="DepartureDate" type="v1:StringDate" />
			<xs:element name="FolioNumber" type="v1:String20max" />
			<xs:element name="ExtraCharges" minOccurs="0" maxOccurs="unbounded">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Gift Shop" />
						<xs:enumeration value="Laundry" />
						<xs:enumeration value="Mini Bar" />
						<xs:enumeration value="Other" />
						<xs:enumeration value="Restaurant" />
						<xs:enumeration value="Telephone" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="NoShowIndicator" type="xs:boolean" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="MerchantDetails">
		<xs:all>
			<xs:element name="ExternalMerchantID" type="v1:String15max">
				<xs:annotation>
					<xs:documentation>Merchant ID that shall be passed to the endpoint (mostly auth switch) if required.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AlternateExternalMerchantID" type="v1:String15max" minOccurs="0" />
			<xs:element name="EndpointID" type="v1:String100max">
				<xs:annotation>
					<xs:documentation>Endpoint ID of the remote system that the transaction request will be sent to.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="v1:Terminal" />
			<xs:element name="MerchantMasterData" type="v1:MerchantMasterData" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Mandatory for the first transaction to the given merchantID or to update the master data for an existing merchant. In case of update please send the complete master data information.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>

	<xs:complexType name="PaymentFacilitator">
		<xs:all>
			<xs:element name="ExternalMerchantID" type="v1:String15max">
				<xs:annotation>
					<xs:documentation>Merchant ID of the payment facilitator.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ID">
				<xs:annotation>
					<xs:documentation>Payment facilitator ID which is supplied during registration process.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="\d{1,11}" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="SaleOrganizationID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Independent Sales Organisation (ISO) ID provided by Mastercard.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="\d{1,11}" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Name" type="v1:String50max" />
		</xs:all>
	</xs:complexType>

	<xs:complexType name="MerchantMasterData">
		<xs:all>
			<xs:element name="Mcc">
				<xs:annotation>
					<xs:documentation>Merchant category code</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:nonNegativeInteger">
						<xs:maxInclusive value="9999" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LegalName" type="v1:String100max">
				<xs:annotation>
					<xs:documentation>store legal name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Dba" type="v1:String50max" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Doing business as</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Email" type="v1:String254max" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Email to send notification</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Url" type="v1:String100max" minOccurs="0">
				<xs:annotation>
					<xs:documentation>store url addrress - (e.g. https://www.webshop.com)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DefaultCurrency" type="v1:CurrencyType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Currency ISO Alpha code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Timezone" type="v1:String50max" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Timezone code (e.g. 'Europe/London', 'Europe/Berlin', 'Brazil/West')</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Address" type="v1:AddressType" />
			<xs:element name="dccEnrollment" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Set to "activate" if the merchant is considered to support DCC transactions. Please note this is a configuration on merchant level, activate should be always set for merchants supporting DCC, even if a non DCC transaction is submitted. At the first time this is set, mind the activation will be forwarded to the DCC service provider, therefore it could take up to a day until DCC is really activated for the merchant. Set to "deactivate" only if the merchant shall be unenrolled at the DCC provider.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="activate" />
						<xs:enumeration value="deactivate" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Document" type="v1:Document" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Document information, only evaluated in context of SubMerchant (related to Payment Facilitator)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MerchantID" type="v1:String15max" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Merchant ID, only evaluated in context of SubMerchant (related to Payment Facilitator). 
					In context of psp for which the Merchant ID shall be passed to the endpoint please use MerchantDetails/ExternalMerchantID</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>

	<xs:complexType name="Document">
		<xs:all>
			<xs:element name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="NATIONAL_IDENTITY" />
						<xs:enumeration value="SINGLE_TAX_IDENTIFICATION" />
						<xs:enumeration value="SINGLE_CODE_OF_LABOR_IDENTIFICATION" />
						<xs:enumeration value="BOOK_ENLISTMENT" />
						<xs:enumeration value="CIVIC_NOTEBOOK" />
						<xs:enumeration value="PASSPORT" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Number" type="v1:String30max" />
		</xs:all>
	</xs:complexType>

	<xs:complexType name="AddressType">
		<xs:annotation>
			<xs:documentation>definition of the address</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="Address1" type="v1:String50max">
				<xs:annotation>
					<xs:documentation>first part of the address - e.g. Unit 7</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Address2" type="v1:String50max" minOccurs="0">
				<xs:annotation>
					<xs:documentation>second part of the address - e.g. Elouera Road 27</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Zip" type="v1:String12max">
				<xs:annotation>
					<xs:documentation>address ZIP code - e.g. 2230</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="City" type="v1:String50max">
				<xs:annotation>
					<xs:documentation>city - e.g. Sydney</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="State" type="v1:String12max" minOccurs="0">
				<xs:annotation>
					<xs:documentation>address State - e.g. Sydney or California</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Country" type="v1:CountryType">
				<xs:annotation>
					<xs:documentation>ISO country code</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>

	<xs:element name="Basket">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ProductStock" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="check" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Item" maxOccurs="100">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ID" type="v1:String128max" />
							<xs:element name="Description" type="v1:String128max" />
							<xs:group ref="v1:ItemAmount" />
							<xs:element name="Quantity" type="xs:int" />
							<xs:element name="Option" minOccurs="0" maxOccurs="100">
								<xs:complexType>
									<xs:all>
										<xs:element name="Name" type="v1:String128max" />
										<xs:element name="Choice" type="v1:String128max" />
									</xs:all>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="Terminal">
		<xs:complexType>
			<xs:all>
				<xs:element name="TerminalID">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="[0-9]{8}" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:all>
		</xs:complexType>
	</xs:element>

	<xs:element name="OfflineApprovalType">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="OFFLINE_APPROVAL" />
				<!-- Offline Decliened : Transaction declined from point of sale  -->
				<xs:enumeration value="OFFLINE_DECLINED" />
				<!-- Fallback: Unable to go online, approval -->
				<xs:enumeration value="FALLBACK" />
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	
	<xs:element name="TransactionOrigin" default="ECI">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="ECI" />
				<xs:enumeration value="MOTO" />
				<xs:enumeration value="RETAIL" />
			</xs:restriction>
		</xs:simpleType>
	</xs:element>

	<xs:element name="Billing">
		<xs:complexType>
			<xs:all>
				<xs:element name="CustomerID" type="v1:String32max" minOccurs="0" />

				<!-- name section - in case that name is not specified name is created in the application in format "firstname surname"  -->
				<xs:element name="Name" type="v1:String96max" minOccurs="0" >
					<xs:annotation>
						<xs:documentation>Full name of the customer, mandatory for SEPA Direct Debit transactions</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Firstname" type="v1:String48max" minOccurs="0" >
					<xs:annotation>
						<xs:documentation>Customer first name - e.g. John (required for KLARNA, required for SEPA Direct Debit transactions when Name is not provided)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Surname" type="v1:String48max" minOccurs="0" >
					<xs:annotation>
						<xs:documentation>Customer surname - e.g. Doe (required for KLARNA, required for SEPA Direct Debit transactions when Name is not provided)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<!-- end of the name section -->

				<xs:element name="Company" type="v1:String96max" minOccurs="0" />

				<!-- address 1 section - in case that address1 is not filled it is created in the application from input StreetName, HouseNumber and HouseExtension -->
				<xs:element name="Address1" type="v1:String96max" minOccurs="0" >
					<xs:annotation>
						<xs:documentation>first part of the address - e.g. Unit 7 (Ignored for KLARNA payment type)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="StreetName" type="v1:String84max" minOccurs="0" >
					<xs:annotation>
						<xs:documentation>required for KLARNA</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="HouseNumber" type="v1:String6max" minOccurs="0">
					<xs:annotation>
						<xs:documentation>required for KLARNA</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="HouseExtension" type="v1:String6max" minOccurs="0">
					<xs:annotation>
						<xs:documentation>required for KLARNA</xs:documentation>
					</xs:annotation>
				</xs:element>
				<!-- end of address1 section -->

				<xs:element name="Address2" type="v1:String96max" minOccurs="0" >
					<xs:annotation>
						<xs:documentation>second part of the address - e.g. Elouera Road 27</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="City" type="v1:String96max" minOccurs="0" />
				<xs:element name="State" type="v1:String96max" minOccurs="0" />
				<xs:element name="Zip" type="v1:String24max" minOccurs="0" />
				<xs:element name="Country" type="v1:String32max" minOccurs="0" >
					<xs:annotation>
						<xs:documentation>For Klarna transactions use ISO 3166 ALPHA-3 code (e.g. DEU, FIN, SWE...) or ISO 3166 ALPHA-2 (e.g. DE, FI, SE...) or ISO 3166 numeric (276, 246, 752)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Phone" type="v1:String32max" minOccurs="0" />
				<xs:element name="Fax" type="v1:String32max" minOccurs="0" />
				<xs:element name="Email" type="v1:String254max" minOccurs="0" />

				<xs:element name="PersonalNumber" type="v1:String20max" minOccurs="0" >
					<xs:annotation>
						<xs:documentation>Customer personal number (required for KLARNA in format: Denmark "ddmm-nnnn", Finland "ppkkvvvvxxxx", Norway "ddmmyynnnnn", Sweden "yymmddnnnn"))</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="BirthDate" type="xs:date" minOccurs="0" >
					<xs:annotation>
						<xs:documentation>required for KLARNA</xs:documentation>
					</xs:annotation>
