	<xs:simpleType name="TDateType">
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{10}|" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CurrencyType">
		<xs:restriction base="xs:string">
			<!-- http://de.wikipedia.org/wiki/ISO_4217 -->
			<!-- http://en.wikipedia.org/wiki/ISO_4217 -->
			<xs:pattern value="[0-9]{3}|[A-Z]{3}" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="AmountValueType">
		<xs:restriction base="xs:decimal">
			<xs:fractionDigits value="3" />
			<xs:totalDigits value="12" />
			<xs:minInclusive value="0" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="ItemAmountValueType">
		<xs:annotation>
			<xs:documentation>amount value type for the items - allows negative amounts</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:fractionDigits value="3" />
			<xs:totalDigits value="12" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="BINType">
		<xs:restriction base="xs:int">
			<xs:totalDigits value="6" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="GenderType">
		<xs:annotation>
			<xs:documentation>gender
				UNKNOWN ... unknown
				MALE ... male
				FEMALE ... femal
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="UNKNOWN" />
			<xs:enumeration value="MALE" />
			<xs:enumeration value="FEMALE" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="SubsequentTransactionType">
		<xs:annotation>
			<xs:documentation>contains possible transaction types for subsequent transactions</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="RETURN" />
			<xs:enumeration value="VOID" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="PositiveNumeric4max">
		<xs:restriction base="xs:int">
			<xs:pattern value="\d{1,4}" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="PositiveNumeric14max">
		<xs:restriction base="xs:long">
			<xs:pattern value="\d{1,14}" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="PositiveNumeric25max">
		<xs:restriction base="xs:long">
			<xs:pattern value="\d{1,25}" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CardAccountType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="saving" />
			<xs:enumeration value="checking" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CardFunctionType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="credit" />
			<xs:enumeration value="debit" />
			<xs:enumeration value="voucher" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="IssuerAuthorizationResponseCode-8A">
		<xs:restriction base="xs:string">
			<xs:length value="2" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Rate">
		<xs:restriction base="xs:decimal">
			<xs:fractionDigits value="2" />
			<xs:minInclusive value="0" />
			<xs:maxInclusive value="99.99" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="RecurringType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="FIRST" />
			<xs:enumeration value="REPEAT" />
			<xs:enumeration value="STANDIN" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="UnscheduledCredentialOnFileType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="FIRST" />
			<xs:enumeration value="CARDHOLDER_INITIATED" />
			<xs:enumeration value="MERCHANT_INITIATED" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="WalletType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="ANDROID_PAY" />
			<xs:enumeration value="APPLE_PAY" />
			<xs:enumeration value="MASTERPASS" />
			<xs:enumeration value="SAMSUNG_PAY" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="TraceNumber-DF8260">
		<xs:restriction base="xs:int">
			<xs:minInclusive value="0" />
			<xs:maxInclusive value="999999" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="TokenType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="ONETIME" />
			<xs:enumeration value="MULTIPAY"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="AssignToken" >
		<xs:restriction base="xs:boolean"/>
	</xs:simpleType>

	<xs:simpleType name="IVRDeviceIdFormat">
		<xs:restriction base="xs:string">
			<xs:enumeration value="I">
				<xs:annotation>
					<xs:documentation>International format</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="D">
				<xs:annotation>
					<xs:documentation>Domestic format</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="IVRUserDataStatus">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Y">
				<xs:annotation>
					<xs:documentation>Entered</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="N">
				<xs:annotation>
					<xs:documentation>Not received</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="T">
				<xs:annotation>
					<xs:documentation>Timed out</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="U">
				<xs:annotation>
					<xs:documentation>Unknown</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="IVRAuthDataName">
		<xs:restriction base="xs:string">
			<xs:enumeration value="SP">
				<xs:annotation>
					<xs:documentation>Static password</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OTP1">
				<xs:annotation>
					<xs:documentation>One Time Password  Issued to
						Cardholder Prior to Entering into
						Transaction</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OTP2">
				<xs:annotation>
					<xs:documentation>One Time Password  Issued to
						Cardholder During the Transaction</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TTP">
				<xs:annotation>
					<xs:documentation>Authentication Performed by a
						Trusted Third Party</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ICB">
				<xs:annotation>
					<xs:documentation>Issuer Call-Back</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="other">
				<xs:annotation>
					<xs:documentation>Other issuer-specific Authentication
						Method (e.g., NETBANKINGPIN)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="IVRAuthDataType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="A">
				<xs:annotation>
					<xs:documentation>Text</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="N">
				<xs:annotation>
					<xs:documentation>Numeric Only</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="IVRShoppingChannel">
		<xs:restriction base="xs:string">
			<xs:enumeration value="IVR">
				<xs:annotation>
					<xs:documentation>Via Interactive Voice Response</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CLIENT">
				<xs:annotation>
					<xs:documentation>Via J2ME (Java2 Micro Edition) or STK application</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TTP">
				<xs:annotation>
					<xs:documentation>Via Trusted Third Party</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SMS">
				<xs:annotation>
					<xs:documentation>Via Short Messaging Service (SMS)
						OR via Unstructured Supplementary
						Service Data (USSD)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WAP">
				<xs:annotation>
					<xs:documentation>Via Wireless Application Protocol</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="native-app">
				<xs:annotation>
					<xs:documentation>Other native app</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="IVRAuthenticationChannel">
		<xs:restriction base="xs:string">
			<xs:enumeration value="SMS">
				<xs:annotation>
					<xs:documentation>Via Short Messaging Service</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IVR">
				<xs:annotation>
					<xs:documentation>Via Interactive Voice Response</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="USSD">
				<xs:annotation>
					<xs:documentation>Via Unstructured Supplementary Service Data</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WAP">
				<xs:annotation>
					<xs:documentation>Via Wireless Application Protocol</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="IbanType">
		<xs:restriction base="xs:string">
			<xs:pattern value="[A-Z]{2}[0-9]{2}[A-Z0-9]{4,30}" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="BankCodeType">
		<xs:restriction base="xs:string">
			<xs:pattern value="([0-9]{8})|([A-Z]{4}DE([A-Z0-9][A-NP-Z0-9])(XXX|[A-WY-Z0-9][A-Z0-9]{2})?)" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="AccountNumberType">
		<xs:restriction base="xs:string">
			<xs:pattern value="([0-9]{1,10})|(DE[0-9]{2}[0-9]{8}[0-9]{10})" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="BICType">
		<xs:restriction base="xs:string">
			<xs:pattern value="[A-Z]{4}[A-Z]{2}[A-Z0-9]{2}(XXX|[A-WY-Z0-9][A-Z0-9]{2})?" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="MandateReferenceType">
			<xs:restriction base="xs:string">
				<xs:pattern value="[A-Za-z0-9+?/\\:().,'-]{1,35}" />
			</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="MandateType">
			<xs:restriction base="xs:string">
				<xs:enumeration value="SINGLE" />
				<xs:enumeration value="RECURRING_COLLECTION" />
				<xs:enumeration value="FIRST_COLLECTION" />
				<xs:enumeration value="FINAL_COLLECTION" />
			</xs:restriction>
	</xs:simpleType>
</xs:schema>
