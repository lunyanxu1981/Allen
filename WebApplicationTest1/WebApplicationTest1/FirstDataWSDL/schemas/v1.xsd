<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:v1="http://ipg-online.com/ipgapi/schemas/v1" targetNamespace="http://ipg-online.com/ipgapi/schemas/v1"
	elementFormDefault="qualified">

	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<!-- main - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->

	<xs:element name="Transaction" type="v1:Transaction" />
	<xs:element name="SubsequentTransaction" type="v1:SubsequentTransaction" />

	<xs:element name="MerchantDetails" type="v1:MerchantDetails">
		<xs:annotation>
			<xs:documentation>Please use MerchantDetails instead of StoreId if you are integrating based on the Payment Provider API.</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:complexType name="AdditionalAmountAndRate">
		<xs:sequence>
			<xs:element name="Amount" type="v1:AmountValueType" />
			<xs:element name="Rate" type="v1:Rate">
				<xs:annotation>
					<xs:documentation>rate of the additional amount, e.g. 0.09 (means 9%)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="SubsequentTransaction">
		<xs:sequence>
			<xs:choice>
				<xs:element name="IpgTransactionId" type="v1:PositiveNumeric14max" />
				<xs:element name="ReferencedMerchantTransactionId" type="v1:String40max" />
				<xs:sequence>
					<xs:element name="OrderId" type="v1:String100max"/>
					<xs:element name="TDate" type="v1:TDateType" />
				</xs:sequence>
			</xs:choice>
			<xs:element name="MerchantTransactionId" type="v1:String40max" minOccurs="0" >
				<xs:annotation>
					<xs:documentation>Specify a unique ID for the transaction which can be used to reference the transaction in the request</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Options" type="v1:Options" minOccurs="0"/>
			<xs:element name="TransactionType" type="v1:SubsequentTransactionType">
				<xs:annotation>
					<xs:documentation>Specify the transaction type which we are going to do (e.g. VOID, RETURN)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="v1:Payment" minOccurs="0" />
			<xs:element ref="v1:Basket" minOccurs="0" >
				<xs:annotation>
					<xs:documentation>Basket items for the subsequent transaction (Will be ignored for the VOID transaction type)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Transaction">
		<xs:sequence>
			<xs:choice>
				<xs:sequence>
					<xs:element name="CreditCardTxType" type="v1:CreditCardTxType" />
					<xs:choice>
						<xs:sequence>
							<xs:element name="CreditCardData" type="v1:CreditCardData" minOccurs="0" />
							<xs:choice>
								<xs:element ref="v1:CreditCard3DSecure" minOccurs="0" />
								<xs:element ref="v1:Upop" minOccurs="0" />
							</xs:choice>
							<xs:element ref="v1:MCC6012Details" minOccurs="0" />
						</xs:sequence>
						<xs:element name="EMVCardPresentRequest" type="v1:EMVCardPresentRequest"/>
					</xs:choice>
					<xs:element name="cardFunction" type="v1:CardFunctionType" minOccurs="0" />
					<xs:element name="cardAccount" type="v1:CardAccountType" minOccurs="0" />
					<xs:element name="recurringType" type="v1:RecurringType" minOccurs="0" />
					<xs:element name="unscheduledCredentialOnFileType" type="v1:UnscheduledCredentialOnFileType" minOccurs="0" />
					<xs:element name="PaymentFacilitator" type="v1:PaymentFacilitator" minOccurs="0">
						<xs:annotation>
							<xs:documentation>If only PaymentFacilitator element is given but no SubMerchant element, the transaction is considered as sub merchant transaction started by sub merchant store. </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SubMerchant" type="v1:MerchantMasterData" minOccurs="0">
						<xs:annotation>
							<xs:documentation>FOR FUTURE USE! Once SubMerchant element is given, the transaction is considered as sub merchant transaction started with master store (aka: Payment Facilitator store). 
							The payment facilitator information should be either given in the PaymentFacilitator element or configured on store level in the master store. </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Wallet" type="v1:Wallet" minOccurs="0" />
				</xs:sequence>
				<xs:sequence>
					<xs:element name="CustomerCardTxType" type="v1:CustomerCardTxType" />
					<xs:element name="CustomerCardData" type="v1:CustomerCardData" minOccurs="0" />
				</xs:sequence>
				<xs:sequence>
					<xs:element name="DE_DirectDebitTxType" type="v1:DE_DirectDebitTxType" />
					<xs:choice>
						<xs:element name="DE_DirectDebitData" type="v1:DE_DirectDebitData" minOccurs="0" />
						<xs:element name="DE_DirectDebitEMV" type="v1:EMVCardPresentRequest"/>
					</xs:choice>
				</xs:sequence>
				<xs:sequence>
					<xs:element name="IdealTxType" type="v1:IdealTxType" />
				</xs:sequence>
				<xs:sequence>
					<xs:element name="PayPalTxType" type="v1:PayPalTxType" />
				</xs:sequence>
				<xs:sequence>
					<xs:element name="SofortTxType" type="v1:SofortTxType" />
				</xs:sequence>
				<xs:sequence>
					<xs:element name="TopUpTxType" type="v1:TopUpTxType" />
				</xs:sequence>
				<xs:sequence>
					<xs:element name="PaymentUrlTxType" type="v1:PaymentUrlTxType" />
				</xs:sequence>
				<xs:sequence>
					<xs:element name="KlarnaTxType" type="v1:KlarnaTxType" />
					<xs:element name="KlarnaPClassID" type="xs:int" minOccurs="0">
						<xs:annotation>
							<xs:documentation>For Invoice please send -1</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:sequence>
					<xs:annotation>
						<xs:documentation>Sepa transaction type is relevant only for Local Payments (APM)</xs:documentation>
					</xs:annotation>
					<xs:element name="SepaTxType" type="v1:SepaTxType"/>
					<xs:element name="SepaData">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="IBAN" type="v1:IbanType" minOccurs="1"/>
								<xs:element ref="v1:Mandate" minOccurs="0"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:choice>
			<xs:element ref="v1:Payment" minOccurs="0" />
			<xs:element name="TransactionDetails" type="v1:TransactionDetails" minOccurs="0" />
			<xs:element ref="v1:Billing" minOccurs="0" />
			<xs:element ref="v1:Shipping" minOccurs="0" />
			<xs:element ref="v1:ClientLocale" minOccurs="0" />
			<xs:element ref="v1:Basket" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<!-- Options - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->

	<xs:complexType name="TopUpTxType">
		<xs:complexContent>
			<xs:extension base="v1:Options">
				<xs:choice>
					<xs:element name="MPCharge">
						<xs:complexType>
							<xs:all>
								<xs:element ref="v1:MNSP" />
								<!-- MobilePhoneNumber -->
								<xs:element name="MSISDN" minOccurs="0">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:pattern value="[0-9]{5,15}" />
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element ref="v1:PaymentType" />
							</xs:all>
						</xs:complexType>
					</xs:element>
				</xs:choice>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="CreditCardTxType">
		<xs:complexContent>
			<xs:extension base="v1:Options">
				<xs:sequence>
					<xs:element name="Type">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="credit" />
								<xs:enumeration value="forceTicket" />
								<xs:enumeration value="forceticket" />
								<xs:enumeration value="postAuth" />
								<xs:enumeration value="postauth" />
								<xs:enumeration value="preAuth" />
								<xs:enumeration value="preauth" />
								<xs:enumeration value="return" />
								<xs:enumeration value="sale" />
								<xs:enumeration value="void" />
								<xs:enumeration value="payerAuth" />
								<xs:enumeration value="payerauth" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="PaymentUrlTxType">
		<xs:complexContent>
			<xs:extension base="v1:Options">
				<xs:sequence>
					<xs:element name="Type">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="preAuth" />
								<xs:enumeration value="preauth" />
								<xs:enumeration value="sale" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="CustomerCardTxType">
		<xs:complexContent>
			<xs:extension base="v1:Options">
				<xs:sequence>
					<xs:element name="Type">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="forceTicket" />
								<xs:enumeration value="forceticket" />
								<xs:enumeration value="BWListCheck" />
								<xs:enumeration value="bwlistcheck" />
								<xs:enumeration value="sale" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="DE_DirectDebitTxType">
		<xs:complexContent>
			<xs:extension base="v1:Options">
				<xs:sequence>
					<xs:element name="Type">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="credit" />
								<xs:enumeration value="return" />
								<xs:enumeration value="sale" />
								<xs:enumeration value="void" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="IdealTxType">
		<xs:complexContent>
			<xs:extension base="v1:Options">
				<xs:sequence>
					<xs:element name="Type">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="return" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="PayPalTxType">
		<xs:complexContent>
			<xs:extension base="v1:Options">
				<xs:sequence>
					<xs:element name="Type">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="credit" />
								<xs:enumeration value="postAuth" />
								<xs:enumeration value="postauth" />
								<xs:enumeration value="return" />
								<xs:enumeration value="void" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="SofortTxType">
		<xs:complexContent>
			<xs:extension base="v1:Options">
				<xs:sequence>
					<xs:element name="Type">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="return" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="KlarnaTxType">
		<xs:complexContent>
			<xs:extension base="v1:Options">
				<xs:sequence>
					<xs:element name="Type">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="preAuth" />
								<xs:enumeration value="preauth" />
								<xs:enumeration value="postAuth" />
								<xs:enumeration value="postauth" />
								<xs:enumeration value="void" />
								<xs:enumeration value="return" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="SepaTxType">
		<xs:complexContent>
			<xs:extension base="v1:Options">
				<xs:sequence>
					<xs:element name="Type">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="credit" />
								<xs:enumeration value="sale" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="Options">
		<xs:sequence>
			<xs:element name="StoreId" type="v1:String20max" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Please submit a Store ID in case you manage multiple Store IDs with your integration.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<!-- Card - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->

	<xs:complexType name="CreditCardData">
		<xs:sequence>
			<xs:choice>
				<xs:sequence>
					<xs:group ref="v1:Card" minOccurs="0" />
					<xs:element name="CardCodeValue" type="v1:CardCodeValue" minOccurs="0" />
					<xs:element ref="v1:CryptData" minOccurs="0" />
				</xs:sequence>
				<xs:element name="TrackData" type="v1:TrackData" />
			</xs:choice>
			<xs:element name="Brand" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="AMEX" />
						<xs:enumeration value="BCMC" />
						<xs:enumeration value="CABAL" />
						<xs:enumeration value="DINERS/DISCOVER" />
						<xs:enumeration value="DINACARD" />
						<xs:enumeration value="JCB" />
						<xs:enumeration value="MAESTRO" />
						<xs:enumeration value="MASTERCARD" />
						<xs:enumeration value="MEXICOLOCAL" />
						<xs:enumeration value="SOROCRED" />
						<xs:enumeration value="VISA" />
						<xs:enumeration value="UNIONPAY" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CustomerCardData">
		<xs:sequence>
			<xs:choice>
				<xs:group ref="v1:Card" />
				<xs:element name="TrackData" type="v1:TrackData" />
			</xs:choice>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="DE_DirectDebitData">
		<xs:sequence>
			<xs:choice minOccurs="0">
				<xs:sequence>
					<xs:element name="BIC" minOccurs="0" type="v1:BICType"/>
					<xs:element name="IBAN" type="v1:IbanType"/>
				</xs:sequence>
				<xs:element name="TrackData" type="v1:TrackData" />

				<!-- deprecated -->
				<xs:sequence>
					<xs:element name="BankCode" type="v1:BankCodeType"/>

					<xs:element name="AccountNumber" type="v1:AccountNumberType"/>
				</xs:sequence>

			</xs:choice>
			<xs:element ref="v1:MandateReference" minOccurs="0" />
			<xs:element ref="v1:MandateType" minOccurs="0" />
			<xs:element name="DateOfMandate" type="v1:StringDate" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="MandateReference" type="v1:MandateReferenceType" />

	<xs:element name="MandateType" default="SINGLE">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="SINGLE" />
				<xs:enumeration value="RECURRING_COLLECTION" />
				<xs:enumeration value="FIRST_COLLECTION" />
				<xs:enumeration value="FINAL_COLLECTION" />
			</xs:restriction>
		</xs:simpleType>
	</xs:element>

	<xs:group name="Card">
		<xs:sequence>
			<xs:element name="CardNumber" type="v1:CardNumber" minOccurs="0" />
			<xs:element name="ExpMonth">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="(0[1-9])|(1[0-2])" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ExpYear">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="[0-9]{2}" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:group>

	<xs:simpleType name="CardCodeValue">
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{3,4}|" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CardNumber">
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{13,19}" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="TrackData">
		<xs:restriction base="xs:string">
			<!-- track 1 length + track 2 length + track 3 length = 90 + 64 + 107 = 261 -->
			<xs:pattern value="[a-zA-Z0-9;.%=^?/ \\-]{1,261}" />
		</xs:restriction>
	</xs:simpleType>

	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<!-- TopUpTxType - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->

	<!-- MobileNetworkServiceProvider -->
	<xs:element name="MNSP">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<!-- Deutsche Telekom -->
				<xs:enumeration value="D1" />
				<!-- Vodafon -->
				<xs:enumeration value="D2" />
				<!-- E+ -->
				<xs:enumeration value="EP" />
				<!-- O2 -->
				<xs:enumeration value="VI" />
			</xs:restriction>
		</xs:simpleType>
	</xs:element>

	<xs:element name="PaymentType">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Amex" />
				<xs:enumeration value="Cash" />
				<xs:enumeration value="Diners" />
				<xs:enumeration value="ECMC" />
				<xs:enumeration value="JCB" />
				<xs:enumeration value="VISA" />
			</xs:restriction>
		</xs:simpleType>
	</xs:element>

	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<!-- Card present - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->

	<xs:complexType name="EMVCardPresentRequest">
		<xs:sequence>
			<xs:element ref="v1:CryptData" minOccurs="0">
				<xs:annotation>
					<xs:documentation>CryptData is only optional for the referenced transaction, for the first EMV transaction like sale, preauth, credit etc. it must be given.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element ref="v1:EMVRequestData" />
				<xs:element ref="v1:TLVData" />
			</xs:choice>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="CryptData">
		<xs:complexType>
			<xs:all>
				<xs:element name="SRED">
					<xs:complexType>
						<xs:all>
							<xs:element name="Value">
								<xs:simpleType>
									<xs:restriction base="xs:hexBinary">
										<!-- 
										<xs:length value="256" /> 
										-->
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="KSN">
								<xs:simpleType>
									<xs:restriction base="xs:hexBinary">
										<xs:length value="10" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:all>
					</xs:complexType>
				</xs:element>
				<xs:element name="PINBlock" minOccurs="0">
					<xs:complexType>
						<xs:all>
							<!-- 
							<xs:element name="Format"> 
								<xs:simpleType> 
									<xs:restriction base="xs:string"> 
										<xs:length value="8" /> 
									</xs:restriction> 
								</xs:simpleType> 
							</xs:element> 
							-->
							<xs:element name="Value">
								<xs:simpleType>
									<xs:restriction base="xs:hexBinary">
										<!-- 
										<xs:length value="256" /> 
										-->
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="KSN">
								<xs:simpleType>
									<xs:restriction base="xs:hexBinary">
